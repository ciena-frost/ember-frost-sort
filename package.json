{
  "name": "ember-frost-sort",
  "version": "7.2.10",
  "description": "A sorting component to sort collections",
  "frostGuideDirectory": "ui-components/content-views/sort",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "coverage": "COVERAGE=true ember test -r 'dot'",
    "lint": "lint-all-the-things",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- ember test"
  },
  "repository": "git@github.com:ciena-frost/ember-frost-sort.git",
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "Olufemi Adeojo (https://github.com/oadeojo)",
  "contributors": [
    "Steven Glanzer (https://github.com/sglanzer)",
    "Roxanne Panthaky https://github.com/rox163)",
    "Quincy Le (https://github.com/quincyle)",
    "Gosia Hyndman (https://github.com/vesper2000)",
    "Matthew Dahl (https://github.com/sandersky)",
    "Michael Carroll (https://github.com/juwara0)"
  ],
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "chai-jquery": "^2.0.0",
    "ember-cli": "2.16.2",
    "ember-cli-chai": "0.4.3",
    "ember-cli-code-coverage": "0.3.5",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-frost-blueprints": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "0.14.4",
    "ember-cli-notifications": "^4.1.6",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-code-snippet": "^2.0.0",
    "ember-concurrency": "~0.8.10",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-elsewhere": "1.0.2",
    "ember-export-application-global": "^2.0.0",
    "ember-frost-test": "^2.1.2",
    "ember-hook": "^1.4.2",
    "ember-load-initializers": "^1.0.0",
    "ember-prop-types": "^4.0.1",
    "ember-resolver": "^4.0.0",
    "ember-simple-uuid": "0.1.4",
    "ember-sinon": "^0.7.0",
    "ember-source": "~2.16.0",
    "ember-spread": "^2.0.1",
    "ember-test-utils": "^6.0.0",
    "ember-truth-helpers": "^2.0.0",
    "eslint-plugin-ember": "^4.5.0",
    "loader.js": "^4.2.3",
    "sinon-chai": "^2.14.0"
  },
  "keywords": [
    "ember-addon",
    "frost",
    "sort"
  ],
  "ember-frost-icon-pack": {
    "name": "frost-sort"
  },
  "dependencies": {
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-sass": "7.0.0",
    "ember-decorators": "1.3.1",
    "ember-frost-core": "^2.0.2"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
